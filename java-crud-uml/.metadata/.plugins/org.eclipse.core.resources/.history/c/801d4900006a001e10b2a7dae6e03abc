package com.agencia.model;

public class Pedidos {

	private int id_Pedidos;
	private String id_Cliente;
	private String nomeDestino;
	private String nomePromocao;
	private String statusPedido;
	private String valorCompra;


	

public Pedidos() {
	super();
	
}




public Pedidos(int id_Cliente, String nomeDestino, String nomePromocao, String statusPedido, String valorCompra) {
	super();
	this.id_Cliente = id_Cliente;
	this.nomeDestino = nomeDestino;
	this.nomePromocao = nomePromocao;
	this.statusPedido = statusPedido;
	this.valorCompra = valorCompra;
}




public int getId_Pedidos() {
	return id_Pedidos;
}




public void setId_Pedidos(int id_Pedidos) {
	this.id_Pedidos = id_Pedidos;
}




public String getId_Cliente() {
	return id_Cliente;
}




public void setId_Cliente(String string) {
	this.id_Cliente = string;
}




public String getNomeDestino() {
	return nomeDestino;
}




public void setNomeDestino(String nomeDestino) {
	this.nomeDestino = nomeDestino;
}




public String getNomePromocao() {
	return nomePromocao;
}




public void setNomePromocao(String nomePromocao) {
	this.nomePromocao = nomePromocao;
}




public String getStatusPedido() {
	return statusPedido;
}




public void setStatusPedido(String statusPedido) {
	this.statusPedido = statusPedido;
}




public String getValorCompra() {
	return valorCompra;
}




public void setValorCompra(String valorCompra) {
	this.valorCompra = valorCompra;
}




@Override
public String toString() {
	return "Pedidos [id_Pedidos=" + id_Pedidos + ", id_Cliente=" + id_Cliente + ", nomeDestino=" + nomeDestino
			+ ", nomePromocao=" + nomePromocao + ", statusPedido=" + statusPedido + ", valorCompra=" + valorCompra
			+ "]";
}

}
